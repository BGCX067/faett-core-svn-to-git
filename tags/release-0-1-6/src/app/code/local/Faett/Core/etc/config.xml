<config>
    <modules>
        <Faett_Core>
            <version>${version}</version>
        </Faett_Core>
    </modules>
    <global>
        <helpers>
            <faettcore>
                <class>Faett_Core_Helper</class>
            </faettcore>
        </helpers>
    </global>
    <adminhtml>
		<acl>
			<resources>
				<admin>
					<children>
						<system>
							<children>
								<config>
									<children>
										<core translate="title" module="faettcore">
											<title>Core</title>
											<sort_order>10</sort_order>
										</core>
									</children>
								</config>
							</children>
						</system>
					</children>
				</admin>
			</resources>
		</acl>
	</adminhtml>
</config>